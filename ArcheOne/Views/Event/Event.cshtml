        <div class="card-header">
            <h3 class="card-title">Event List</h3>
            <input type="button" id="AddEvent" class="btn btn-primary float-right" value="Add Event" />
        </div>
        <div class="card-body">
            <div id="divEventList">
            </div>
        </div>
    </div>
</section>

@section Scripts{
    <script src="~/archeone/js/Event/event.js"></script>
}

